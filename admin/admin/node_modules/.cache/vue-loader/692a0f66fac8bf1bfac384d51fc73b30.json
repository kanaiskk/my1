iv class=\"input-icon\">\r\n            <i class=\"fa fa-user-circle\"></i>\r\n          </div>\r\n          <el-select v-model=\"loginForm.tableName\" placeholder=\"请选择角色：\" @change=\"selectChange\">\r\n            <el-option v-for=\"item,index in roles\" :key=\"index\" :label=\"item.roleName\" :value=\"item.tableName\" />\r\n          </el-select>\r\n        </div>\r\n        \r\n        <!-- 按钮 -->\r\n        <div class=\"list-btn\">\r\n          <el-button class=\"login_btn\" v-if=\"loginType==1\" @click=\"submitForm('loginForm')\">\r\n            <i class=\"fa fa-sign-in mr-2\"></i>登录\r\n          </el-button>\r\n          \r\n          <div class=\"list-btn2\">\r\n            <router-link class=\"register_btn\" \r\n              :to=\"{path: '/register', query: {role: item.tableName,pageFlag:'register'}}\" \r\n              v-if=\"item.hasFrontRegister=='是'\" \r\n              v-for=\"(item, index) in roles\" \r\n              :key=\"index\">\r\n              <i class=\"fa fa-user-plus mr-1\"></i>注册{{item.roleName.replace('注册','')}}\r\n            </router-link>\r\n            \r\n            <router-link class=\"resetpwd_btn\" :to=\"{path: '/register', query: {pageFlag:'security1'}}\">\r\n              <i class=\"fa fa-key mr-1\"></i>重置密码\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport 'animate.css';\r\nimport menu from '@/config/menu'\r\nexport default {\r\n  // 保持原有JavaScript代码不变...\r\n  data() {\r\n    return {\r\n      baseUrl: this.$config.baseUrl,\r\n      loginType: 1,\r\n      roleMenus: [],\r\n      loginForm: {\r\n        username: '',\r\n        password: '',\r\n        tableName: '',\r\n      },\r\n      role: '',\r\n      roles: [],\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入账号', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' }\