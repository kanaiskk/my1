px rgba(0, 0, 0, 0.2);\r\n          color: #DC9D1F;\r\n          font-weight: 600;\r\n          width: 100%;\r\n          font-size: 22px;\r\n          line-height: 1.3;\r\n        }\r\n      }\r\n      \r\n      .list-item {\r\n        border-radius: 25px;\r\n        padding: 0;\r\n        margin: 0 0 25px 0;\r\n        background: rgba(255, 255, 255, 0.2); /* 半透明输入框背景 */\r\n        display: flex;\r\n        width: 100%;\r\n        position: relative;\r\n        transition: all 0.3s ease;\r\n        \r\n        &:hover {\r\n          transform: translateY(-2px);\r\n          box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\r\n        }\r\n        \r\n        .input-icon {\r\n          width: 45px;\r\n          height: 45px;\r\n          display: flex;\r\n          justify-content: center;\r\n          align-items: center;\r\n          color: #DC9D1F;\r\n          font-size: 18px;\r\n          background: rgba(255, 255, 255, 0.1);\r\n          border-radius: 25px 0 0 25px;\r\n        }\r\n        \r\n        input {\r\n          border: none;\r\n          padding: 0 15px;\r\n          outline: none;\r\n          color: #fff;\r\n          flex: 1;\r\n          background: transparent;\r\n          width: 100%;\r\n          font-size: 16px;\r\n          height: 45px;\r\n          \r\n          &::placeholder {\r\n            color: rgba(255, 255, 255, 0.7);\r\n            font-size: 15px;\r\n          }\r\n        }\r\n        \r\n        .password-box {\r\n          display: flex;\r\n          width: 100%;\r\n          position: relative;\r\n          \r\n          .toggle-password {\r\n            position: absolute;\r\n            right: 15px;\r\n            top: 50%;\r\n            transform: translateY(-50%);\r\n            color: rgba(255, 255, 255, 0.7);\r\n            cursor: pointer;\r\n            font-size: 16px;\r\n            transition: color 0.3s ease;\r\n            \r\n            &:hover {\r\n              color: #DC9D1F;\r\n            }\r\n          }\r\n        }\r\n      }\r\n      \r\n      .select {\r\n        border-radius: 25px;\r\n        padding: 0;\r\n        margin: 0 0 25px 0;\r\n        background: rgba(255, 255, 255, 0.2);\r\n        display: flex;\r\n        width: 100%;\r\n        position: relative;\r\n        transition: all 0.3s ease;\r\n        \r\n        &:hover {\r\n          transform: translateY(-2px);\r\n          box-shadow: 0 5px 15px rgba(0, 0, 0, 0.1);\r\n        }\r\n        \r\n        .input-icon {\r\n          width: 45px;\r\n          height: 45px;\r\n          display: flex;\r\n          justify-content: center;\r\n          align-items: center;\r\n          color: #DC9D1F;\r\n          font-size: 18px;\r\n          background: rgba(255, 255, 255, 0.1);\r\n          border-radius: 25px 0 0 25px;\r\n        }\r\n        \r\n        .el-select {\r\n          width: 100%;\r\n          \r\n          .el-input__inner {\r\n            border: none;\r\n            padding: 0 15px;\r\n            box-shadow: none;\r\n            color: #fff;\r\n            background: transparent;\r\n            width: 100%;\r\n            font-size: 16px;\r\n            height: 45px;\r\n            border-radius: 0 25px 25px 0;\r\n            \r\n            &::placeholder {\r\n              color: rgba(255, 255, 255, 0.7);\r\n              font-size: 15px;\r\n            }\r\n          }\r\n          \r\n          .el-input__suffix {\r\n            color: rgba(255, 255, 255, 0.7);\r\n          }\r\n        }\r\n        \r\n        /* 下拉菜单样式 */\r\n        .el-select-dropdown {\r\n          background: rgba(255, 255, 255, 0.9);\r\n          border-radius: 15px;\r\n          box-shadow: 0 5px 20px rgba(0, 0, 0, 0.2);\r\n          \r\n          .el-select-dropdown__item {\r\n            color: #333;\r\n            \r\n            &.hover, &:hover {\r\n              background: rgba(220, 157, 31, 0.1);\r\n              color: #DC9D1F;\r\n            }\r\n            \r\n            &.selected {\r\n              color: #DC9D1F;\r\n              font-weight: 600;\r\n            }\r\n          }\r\n        }\r\n      }\r\n      \r\n      .list-btn {\r\n        margin: 0;\r\n        width: 100%;\r\n        \r\n        .login_btn {\r\n          border: none;\r\n          cursor: pointer;\r\n          border-radius: 25px;\r\n          padding: 0 30px;\r\n          margin: 0;\r\n          outline: none;\r\n          color: #fff;\r\n          background: linear-gradient(90deg, #DC9D1F 0%, #E6B44A 100%);\r\n          width: 100%;\r\n          font-size: 16px;\r\n          font-weight: 600;\r\n          height: 48px;\r\n          transition: all 0.3s ease;\r\n          box-shadow: 0 5px 15px rgba(220, 157, 31, 0.2);\r\n          \r\n          &:hover {\r\n            transform: translateY(-2px);\r\n            box-shadow: 0 8px 20px rgba(220, 157, 31, 0.3);\r\n            opacity: 0.95;\r\n          }\r\n          \r\n          &:active {\r\n            transform: translateY(1px);\r\n          }\r\n        }\r\n        \r\n        .list-btn2 {\r\n          margin: 20px 0 0;\r\n          width: 100%;\r\n          display: flex;\r\n          flex-wrap: wrap;\r\n          justify-content: center;\r\n          \r\n          .register_btn, .resetpwd_btn {\r\n            cursor: pointer;\r\n            margin: 8px 15px;\r\n            color: rgba(255, 255, 255, 0.8);\r\n            background: none;\r\n            text-decoration: none;\r\n            font-size: 14px;\r\n            transition: all 0.3s ease;\r\n            \r\n            &:hover {\r\n              color: #DC9D1F;\r\n              transform: translateY(-1px);\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n/* 响应式设计 */\r\n@media (max-width: 576px) {\r\n  .login-container .login_form {\r\n    padding: 30px 20px;\r\n    width: 95%;\r\n  }\r\n  \r\n  .login-container .login-title .title-main {\r\n    font-size: 24px;\r\n  }\r\n  \r\n  .login-container .login-title .title-sub {\r\n    font-size: 20px;\r\n  }\r\n}\r\n\r\n/* 图标间距辅助类 */\r\n.mr-1 {\r\n  margin-right: 5px;\r\n}\r\n\r\n.mr-2 {\r\n  margin-right: 8px;\r\n}\r\n</style>"]}]}